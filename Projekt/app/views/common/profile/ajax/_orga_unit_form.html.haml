.edit_user
  = form_for :user, :url => add_orga_unit_athletes_path, :remote => true do |f|
    .field
      = fields_for :organization_unit do |o|
        = o.autocomplete_field :name, autocomplete_orga_unit_name_path, :value => 'Vereinsname'
    .actions
      = f.submit 'Speichern'
      = link_to image_tag('abbrechen.png'), '', 'data-edit' => 'abort'